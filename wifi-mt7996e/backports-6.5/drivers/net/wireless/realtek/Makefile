obj-$(CPTCFG_RTL8180)		+= rtl818x/
obj-$(CPTCFG_RTL8187)		+= rtl818x/
obj-$(CPTCFG_RTLWIFI)		+= rtlwifi/
obj-$(CPTCFG_RTL8XXXU)		+= rtl8xxxu/
obj-$(CPTCFG_RTW88)		+= rtw88/
obj-$(CPTCFG_RTW89)		+= rtw89/

